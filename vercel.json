{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_GOOGLE_SHEETS_URL": "@google_sheets_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_GOOGLE_SHEETS_URL": "@google_sheets_url"
    }
  }
}